    <fragment
        android:id="@+id/show_parkScooterFragment"
        android:name="dk.itu.moapd.scootersharing.coha.fragments.ParkScooterFragment"
        android:label="ParkScooterFragment" />
    <fragment
        android:id="@+id/show_mlkitBarcodeScannerFragment"
        android:name="dk.itu.moapd.scootersharing.coha.fragments.MlkitBarcodeScannerFragment"
        android:label="fragment_mlkit_barcode_scanner"
        tools:layout="@layout/fragment_mlkit_barcode_scanner" />


</navigation>